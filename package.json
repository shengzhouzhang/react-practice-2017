{
  "name": "react-practice-2017",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Steven Zhang <steven.shengzhou@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "run-p webpack-dev server-dev",
    "webpack-dev": "webpack-dev-server",
    "server-dev": "nodemon src/server/server.js --exec babel-node",
    "mongo": "docker run --rm -p 27017:27017 -it mongodb",
    "mongo-start": "docker run --rm -v db:/data/db -p 27017:27017 -it mongodb",
    "test": "run-s unit-test e2e-test",
    "unit-test": "mocha --opts .mocharc ./src/**/*Spec.*",
    "e2e": "mocha --opts .mocharc ./e2e/**/*Spec.*",
    "e2e-test": "run-p mongo server-dev e2e-wait",
    "e2e-docker": "docker-compose -f e2e/docker-compose.yml run e2e --rm",
    "e2e-wait": "sleep 5 && yarn e2e",
    "start-prod": "webpack && babel-node src/server/server.js"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chai": "^3.5.0",
    "cheerio": "^1.0.0-rc.1",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "mongodb": "^2.2.27",
    "node-sass": "^4.5.3",
    "page": "^1.7.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.6",
    "sinon": "^2.1.0",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "flow-bin": "^0.45.0",
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^2.4.5",
    "yarn-run-all": "^3.1.1"
  }
}
